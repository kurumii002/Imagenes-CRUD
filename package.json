{
  "name": "imagenes-crud",
  "version": "1.0.0",
  "description": "App tipo crud de solo imagenes con TS, MongoDB y Cloudinary",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "compile": "npx tsc",
    "clean": "rimraf ./dist",
    "copydir:views": "ncp ./src/views/ ./dist/views/",
    "build": "npm run clean && npm run compile && npm run copydir:views",
    "start": "node dist/index.js",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix ."
  },
  "keywords": [
    "TypeScript"
  ],
  "author": "Jamutaq Ortega",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/formidable": "^1.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cloudinary": "^1.27.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.4",
    "mongoose": "^6.0.9",
    "multer": "^1.4.3",
    "multer-storage-cloudinary": "^4.0.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2"
  }
}
